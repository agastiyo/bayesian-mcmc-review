@article{ruaneDeepImagingSearch2017,
  title = {Deep {{Imaging Search}} for {{Planets Forming}} in the {{TW Hya Protoplanetary Disk}} with the {{Keck}}/{{NIRC2 Vortex Coronagraph}}},
  author = {Ruane, G. and Mawet, D. and Kastner, J. and Meshkat, T. and Bottom, M. and Castell{\'a}, B. Femen{\'i}a and Absil, O. and Gonzalez, C. Gomez and Huby, E. and Zhu, Z. and {Jensen-Clem}, R. and Choquet, {\'E}. and Serabyn, E.},
  year = 2017,
